{
  "workbench.activityBar.visible": true,
  "editor.fontSize": 18,
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "hidden",
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.rulers": [80, 120],
  "editor.wrappingIndent": "same",
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 80,
  "editor.tabSize": 2,
  "workbench.editor.enablePreview": false,
  "files.trimTrailingWhitespace": true,
  "editor.formatOnType": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSaveTimeout": 5000,
  "explorer.confirmDragAndDrop": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "ruby.useBundler": false, //run non-lint commands with bundle exec
  "ruby.useLanguageServer": true, // use the internal language server (see below)
  "ruby.lint": {
    "rubocop": {
      "useBundler": false // enable rubocop via bundler
    },
    "reek": {
      "useBundler": false // enable reek via bundler
    }
  },
  "ruby.format": "rubocop",
  "editor.codeActionsOnSave": {
    // For ESLint
    "source.fixAll.eslint": false,
    // For TSLint
    "source.fixAll.tslint": false,
    // For Stylelint
    "source.fixAll.stylelint": false
  },
  "ruby.codeCompletion": "rcodetools",
  "[ruby]": {
    "editor.defaultFormatter": "rebornix.ruby",
    "editor.formatOnSave": true
  },
  "[plaintext]": {
    "editor.formatOnSave": false
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.showExpandedAbbreviation": "never",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.suggest.localityBonus": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.fontFamily": "'Ubuntu Mono', Consolas, 'JetBrains Mono for Powerline', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "githubPullRequests.showInSCM": true,
  "vscodeWorkspaceSwitcher.paths": ["/home/leo/projects/vscode"],
  "editor.renderIndentGuides": false,
  "workbench.sideBar.location": "right",
  "workbench.preferredLightColorTheme": "Atom One Light",
  "workbench.preferredDarkColorTheme": "Dracula Soft",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.enabled": false,
  "debug.onTaskErrors": "debugAnyway",
  "typescript.suggest.autoImports": false,
  "javascript.suggest.autoImports": false,
  "files.exclude": {
    ".history": true,
    ".git": true,
    "out": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/.history": true
  },
  "search.followSymlinks": false,
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "locations.extensions": {
    "shagabutdinov.blocks": true,
    "shagabutdinov.nesting": true,
    "shagabutdinov.locations-regexp": true
  },
  "context.debug": false,
  "context.extensions": {
    "shagabutdinov.nesting": true,
    "shagabutdinov.locations": true,
    "shagabutdinov.scope": true
  },
  "commands.extensions": {
    "shagabutdinov.locations": true
  },
  "tabnine.experimentalAutoImports": true,
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "devtools",
    "disabledgrey",
    "displayability",
    "formik",
    "iterrows",
    "mdash",
    "numpy",
    "ohlcv",
    "postgres",
    "prtodo",
    "quantile",
    "reindent",
    "rerender",
    "searchandizing",
    "shagabutdinov",
    "sklearn",
    "snakeize",
    "subheadlines",
    "typeof",
    "xgboost"
  ],
  "rest-client.previewResponseInUntitledDocument": true,
  "rest-client.enableTelemetry": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "devtools.snippets.exclude": [
    "typescript",
    "typescript.snippets.definitions"
  ],
  "editor.quickSuggestionsDelay": 1,
  "editor.snippetSuggestions": "none",
  "editor.suggest.filterGraceful": false,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.showSnippets": false,
  "python.languageServer": "Default",
  "cSpell.language": "en,ru2",
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.showStartPage": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.tabSize": 4,
    "editor.wordWrapColumn": 120,
  },
  "editor.formatOnSave": true,
  "local-history.path": "/home/leo/.local/history",
  "local-history.saveDelay": 5,
  "local-history.treeLocation": "localHistory",
  "vscodeWorkspaceSwitcher.showTreeView": true,
  "go.toolsManagement.autoUpdate": true,
  "explorer.confirmDelete": false,
  "[jsonc]": {
    "editor.defaultFormatter": "shagabutdinov.format"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "redhat.telemetry.enabled": false,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "python.autoComplete.addBrackets": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "eslint.format.enable": true,
  "eslint.enable": true,
  "terminal.integrated.tabs.enabled": true,
  "editor.renderWhitespace": "all",
  "python.formatting.provider": "black",
  "workbench.colorTheme": "Atom One Light",
  "sundial.sunset": "17:00",
  "sundial.sunrise": "5:00",
  "keyboard.dispatch": "keyCode",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true
  },
  "window.zoomLevel": -1
}
