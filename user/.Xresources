URxvt*background:              #000000
URxvt.background:              #000000
URxvt*foreground:              #c0c0c0
URxvt*fading:                  0
URxvt*fadeColor:               #000000
URxvt*cursorColor:             #93a1a1
URxvt*pointerColorBackground:  #586e75
URxvt*pointerColorForeground:  #93a1a1
URxvt*termName: rxvt-unicode
URxvt.scrollBar: false
! urxvt*font:        -xos4-terminus-medium-*-*-*-16-*-*-*-*-*-*-*
urxvt*font:        -xos4-terminus-medium-*-*-*-20-*-*-*-*-*-*-*
! urxvt*font:        -xos4-terminus-bold-*-*-*-28-*-*-*-*-*-*-*
! urxvt*boldFont:    -xos4-terminus-bold-*-*-*-16-*-*-*-*-*-*-*
urxvt*boldFont:    -xos4-terminus-bold-*-*-*-20-*-*-*-*-*-*-*
! rxvt*boldFont:    -xos4-terminus-bold-*-*-*-28-*-*-*-*-*-*-*

URxvt*modifier: mod1
URxvt.perl-ext-common:      default,url-select,keyboard-select,matcher,clipboard,autocomplete-ALL-the-things

URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true
URxvt.keysym.M-q:           perl:url-select:select_next
URxvt.keysym.M-e:           perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.clipboard.autocopy:   true

URxvt.url-launcher: /usr/bin/chromium
URxvt.matcher.button:   1

URxvt.intensityStyles: false

URxvt.keysym.M-w: autocomplete-ALL-the-things:WORD-complete
URxvt.keysym.M-W: autocomplete-ALL-the-things:skeleton-complete
Xcursor.size: 16

URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.saveLines:         20000
